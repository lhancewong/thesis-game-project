config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Restaurant",
"language": "GDScript",
"path": "res://scripts/resources/restaurant.gd"
} ]
_global_script_class_icons={
"Restaurant": ""
}

[application]

config/name="Thesis-game-project"
